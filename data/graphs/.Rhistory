open_data <- read_table2("open.csv",
    locale = locale(decimal_mark = ","))
# open_data <- read.table("open.dat", header=TRUE, sep="\t")
max_y <- max(open_data)
plot_colors <- c("blue","red","forestgreen")
png(filename="open.png", height=295, width=300, bg="white")
plot(open_data$mp4, type="o", col=plot_colors[1],
   ylim=c(0,max_y), axes=FALSE, ann=FALSE)
axis(1, at=1:5, lab=c("1mb", "3mb", "5mb", "10mb"))
axis(2, las=1, at=4*0:max_y)
box()
lines(open_data$txt, type="o", pch=22, lty=2,
   col=plot_colors[2])
# Graph suvs with green dotted line and diamond points
lines(open_data$png, type="o", pch=23, lty=3,
   col=plot_colors[3])
# Create a title with a red, bold/italic font
title(main="Abierto", col.main="red", font.main=4)
title(xlab= "Tiempo", col.lab=rgb(0,0.5,0))
title(ylab= "Tama単o", col.lab=rgb(0,0.5,0))
# Create a legend at (1, max_y) that is slightly smaller
# (cex) and uses the same line colors and points used by
# the actual plots
legend(1, max_y, names(open_data), cex=0.8, col=plot_colors,
   pch=21:23, lty=1:3);
# Turn off device driver (to flush output to png)
dev.off()
